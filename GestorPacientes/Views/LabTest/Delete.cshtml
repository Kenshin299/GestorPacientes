@model GestorPacientes.Core.Application.ViewModels.LabTest.SaveLabTestViewModel

<div class="d-flex flex-column justify-content-center align-items-center pt-3">
    <p>Are you sure you want to delete this LabTest?</p>
    <form asp-action="Delete" asp-route-id="@Model.Id" method="post">
        <button type="submit" class="btn btn-danger">Accept</button>
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>
